    <description></description>
    <display-name>WeatherApp</display-name>
    <servlet-name>WeatherApp</servlet-name>
    <servlet-class>weatherPack.WeatherApp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WeatherApp</servlet-name>
    <url-pattern>/WeatherApp</url-pattern>
  </servlet-mapping>
